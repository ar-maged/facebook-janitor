{
  "name": "facebook-janitor",
  "version": "0.1.0",
  "description": "Delete your status updates back in '07",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/index.ts",
    "build": "tsc"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "puppeteer": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^13.1.6",
    "@types/puppeteer": "^2.0.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.1",
    "typescript": "^3.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,json,css,md}": [
      "prettier --single-quote --trailing-comma all --write",
      "git add"
    ]
  }
}
