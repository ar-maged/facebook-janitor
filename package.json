{
  "name": "facebook-janitor-cli",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/index.ts",
    "build": "tsc"
  },
  "dependencies": {
    "puppeteer": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^12.7.4",
    "@types/puppeteer": "^2.0.0",
    "nodemon": "^1.19.2",
    "prettier": "^1.18.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,json,css,md}": [
      "prettier --single-quote --trailing-comma all --write",
      "git add"
    ]
  }
}
